<%= form_tag standard_station_estimation_stations_path , :method => "get" do %>

<div ><%= text_field_tag "project_id",params[:id] %></div>
 <%#= render :text => params[:id] %>
<table class="table">
	<thead>
	<tr>
		<th><%#= check_box_tag 'checkall', :id =>"checkall" %></th>
		<th><%= submit_tag "Submit Station" , :class=>"btn" %></th>
		<th>No</th>
		<th>Name</th>
		<th></th>
	</tr>
	</thead>
	
	<tbody>
		<% list_standard_stations.stations.each_with_index do |station , index| %>
		<tr>
			<td></td>
			<td><%= check_box_tag "station_check_box[#{station.id}]" %></td>
			<td><%= index + 1 %></td>
			<td><%= station.name %></td>
			<td></td>
		</tr>
	</tbody>
	<% end %> <!-- form tag each do -->
	
</table>
	<%#= submit_tag "Submit Station" , :class=>"button" %>


<% end %> <!-- simple form end -->